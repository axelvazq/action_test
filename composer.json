			  "dist": {
				  "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.11.1.zip",
				  "type": "zip"
			  }
		  }
		},
		{
		  "type": "package",
		  "package": {
			  "name": "ckeditor-plugin/justify",
			  "version": "4.14.1",
			  "type": "drupal-library",
